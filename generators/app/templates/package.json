{
  "name": "<%= name %>",
  "version": "1.0.0",
  "description": "<%= description %>",
  "main": "index.js",
  "private": true,
  "scripts": {
    "build": "run-p build:*",
    "build:babel": "babel -d lib src",
    "build:webpack": "webpack --progress --color",
    "lint": "exlint src",
    "open": "open-cli http://localhost:8000",
    "publish": "run-s -l publish:clean publish:build",
    "publish:clean": "rimraf lib/* public/assets/*",
    "publish:build": "cross-env cross-env NODE_ENV=production npm run build",
    "start": "node .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "run-p -l watch:*",
    "watch:eslint": "chokidar src/**/*.js src/**/*.jsx -c \"eslint --color src\"",
    "watch:babel": "babel -d lib src --watch",
    "watch:webpack": "webpack --progress --color --watch",
    "watch:server": "nodemon -w lib -w public -x \"npm start\""
  },
  "author": "<%= author %>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "chokidar-cli": "^1.2.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "flow-bin": "^0.36.0",
    "flowtype-loader": "^0.2.2",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.2",
    "open-cli": "^1.0.5",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.4.0",
    "webpack": "^1.13.3",
    "webpack-notifier": "^1.4.1"
  },
  "dependencies": {
    "config": "^1.24.0",
    "cross-env": "^3.1.3",
    "express": "^4.14.0",
    "lodash": "^4.17.2",
    "log4js": "^1.0.1",
    "nib": "^1.1.2",
    "pug": "^2.0.0-beta6",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-actions": "^1.1.0"
  }
}
